{
  "name": "store_server_node",
  "version": "1.0.0",
  "description": "Store Server with Node JS",
  "main": "index.js",
  "repository": "https://github.com/borismora/store_server_node.git",
  "author": "bmoraibarra <bmoraibarra@gmail.com>",
  "license": "MIT",
  "keywords": [
    "store",
    "JS",
    "javascript",
    "Express"
  ],
  "scripts": {
    "dev": "nodemon src/index.js",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "jsonwebtoken": "9.0.2",
    "moment": "2.30.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.9.2",
    "pg": "8.11.3",
    "sequelize": "6.37.1",
    "uuid": "9.0.1",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "eslint": "8.57.0",
    "nodemon": "3.1.0",
    "sequelize-cli": "6.6.2",
    "sucrase": "3.35.0"
  }
}
